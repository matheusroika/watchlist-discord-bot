    "axios": "^0.21.1",
    "body-parser": "^1.20.0",
    "date-fns": "^2.21.1",
    "discord-api-types": "^0.27.2",
    "discord.js": "^13.6.0",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "mongoose": "^5.12.11",
    "mustache": "^4.2.0",
    "node-cron": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mustache": "^4.1.1",
    "@types/node": "^15.0.1",
    "@types/node-cron": "^2.0.3",
    "node-env-run": "^4.0.2",
    "nodemon": "^2.0.16",
    "ts-node": "^10.5.0",
    "typescript": "^4.6.2"
  },
